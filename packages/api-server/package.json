{
  "license": "MIT",
  "private": "true",
  "name": "@dailei/api-server",
  "exports": "./dist/index.js",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,mjs,json}\"",
    "eslint": "eslint  --cache \"src/**/*.{js,ts,jsx,tsx,mjs}\"",
    "clean": "rimraf dist && mkdirp dist",
    "prepare": "yarn clean && node ../../puppeteer-support-esm.js",
    "dev": "yarn prepare && NODE_ENV=development nodemon src/index.js",
    "build": "yarn prepare && cp .env* dist/ && yarn rollup --no-treeshake.propertyReadSideEffects -c ../../rollup.config.js",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "bufferutil": "^4.0.3",
    "dotenv-defaults": "^2.0.1",
    "graphql": "^15.5.0",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.7.1",
    "postgres": "^2.0.0-beta.6",
    "puppeteer-core": "^10.1.0",
    "utf-8-validate": "^5.0.5"
  }
}
